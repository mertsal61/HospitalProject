@using EntityLayer.Concrete
@model List<Reports>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Rapor Adı</th>
        <th>Rapor Tarihi</th>
        <th>Sil</th>
        <th>Güncelle</th>
        <th>EKLE</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.reportId</td>
            <td>@item.reportname</td>
            <td>@item.reportdate</td>
            <td><a href="/Report/RaporSil/@item.reportId" class="btn btn-outline-danger"></a>Sil</td>
            <td><a href="/Report/RaporGüncelle/@item.reportId" class="btn btn-outline-success"></a>Güncelle</td>
            <td><a href="/Report/RaporYukle/@item." class="btn btn-outline-success"></a>EKLE</td>
            
        </tr>
    }
</table>